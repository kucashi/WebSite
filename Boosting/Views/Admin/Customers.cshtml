@using Boosting.Models
@model  IEnumerable<OrdersModel>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <table cellpadding="0" cellspacing="0">
        @foreach (OrdersModel customer in Model)
        {
            <tr>
                <td>@customer.battleNetUser</td>
                <td>@customer.battleNetPassword</td>
            </tr>
        }
    </table>
</body>
</html>